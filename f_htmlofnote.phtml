function HtmlOfNote($note)
   {
   //
   $addendum_indicator = "<br>:<br>";
   $addendum_indicator_len = strlen($addendum_indicator);
   if (substr($note,0,$addendum_indicator_len) == $addendum_indicator)
      {
      $note = substr($note,$addendum_indicator_len);
      }
   //
   return $note;
   }
?>
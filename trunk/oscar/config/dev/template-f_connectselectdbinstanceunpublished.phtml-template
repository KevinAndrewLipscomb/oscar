function ConnectSelectDbInstanceUNPUBLISHED(&$db_link)
   {
   $saved_error_level = error_reporting(E_ALL);
   $db_link = mysqli_init();
   $success = $db_link->real_connect('127.0.0.1','ki-oscar-d','~ki-oscar-d~password~','oscar_d',null,null,MYSQLI_CLIENT_SSL|MYSQLI_CLIENT_COMPRESS);
   $success = $success and (mysqli_connect_error() == NULL);
   error_reporting($saved_error_level);
   return $success;
   }
?>

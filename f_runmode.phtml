function RunMode($the_php_self)
   {
   $saved_error_level = error_reporting(E_ALL);
   //
   $run_mode = "";
   if (substr($the_php_self,0,3) == "/x/") $run_mode = "/x/";
   //
   error_reporting($saved_error_level);
   return $run_mode;
   }
?>